    <h1 class="m-1 p-1">Tour Sign Up</h1>
    <h4>Please choose a date and time for your tour.</h4>
</div>


<table class="table-outlined">
    <tr>
        <th>Monday</th>
        <th>Tuesday</th>
        <th>Wednesday</th>
        <th>Thursday</th>
        <th>Friday</th>
        <th>Saturday</th>
        <th>Sunday</th>
    </tr>
    <tr>
        @*@foreach (var t in Model)
        {
            <td><button>@t.AppointmentTime</button></td>
        }*@
    </tr>
</table>
